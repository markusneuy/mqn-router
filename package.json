{
  "name": "mqn-router",
  "version": "0.0.1",
  "description": "A very simple router or pubsub.",
  "main": "dst/index.js",
  "scripts": {
    "start" : "node dst/index.js",
    "pretest": "babel src -d test",
    "test": "set NODE_ENV=test && mocha --recursive -R spec ./test/**/*.spec.js",
    "build": "babel src --ignore **/*.spec.js -d dst && babel src --presets es2015 --plugins transform-es2015-modules-amd --ignore **/*.spec.js -o dst/index.amd.js",
    "docs": "jsdoc src -r -d docs -p",
    "validate": "eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markusneuy/mqn-router.git"
  },
  "keywords": [
    "router",
    "pubsub"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "author": "'Markus <publicmqn@gmx.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/markusneuy/mqn-router/issues"
  },
  "homepage": "https://github.com/markusneuy/mqn-router#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-es2015-modules-amd": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-things": "^0.2.0",
    "eslint": "^3.4.0",
    "jsdoc": "^3.4.0",
    "mocha": "^3.0.2",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0"
  }
}
